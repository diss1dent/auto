app.factory("authHttpResponseInterceptor",["$q","$location","$window","$log","copartUser","urlStore","HttpPendingRequestsService",function($q,$location,$window,$log,copartUser,urlStore,HttpPendingRequestsService){return{response:function(response){var contentPath=location.pathname;if(urlStore.protectedUrls.indexOf(contentPath)>-1)$location.url("/login");return response||$q.when(response)},responseError:function(rejection){if(rejection.status===401){jQuery(".modal-backdrop").remove();copartUser.config=
copartUser.refreshAndGetConfig();HttpPendingRequestsService.cancelAll();$window.location.href=urlStore.login+"?redirectUrl\x3d"+$location.path()}else if(rejection.status===403)jQuery("#UnAuthorizedAccessModal").modal("show");else if(rejection.status===500)try{var jsonObj=rejection.data&&rejection.data.status?rejection.data:JSON.parse(rejection.data);var message=jsonObj["message"];if(jsonObj.status===460||jsonObj.status=="460"){HttpPendingRequestsService.cancelAll();$window.location.href="/termsAndConditions"}else if(jsonObj.status===
461||jsonObj.status=="461"){HttpPendingRequestsService.cancelAll();$window.location.href="/redirectToPaymentGateway/renewal"}else{$log.error("500 ERROR");console.log(rejection);$location.path("/unknown-error")}}catch(e){console.error(e)}else if(rejection.status===503){$log.error("503 ERROR");console.log(rejection);$location.path("/unknown-error")}return $q.reject(rejection)}}}]);
app.factory("templateInterceptor",["$q",function($q){return{response:function(response){return response||$q.when(response)},responseError:function(rejection){return $q.reject(rejection)}}}]);var waitForCLDRInitialization={cldrData:function($q){var deferred=$q.defer();if(app.globalizeNumberFormatter)deferred.resolve(app.globalizeNumberFormatter);else app.globalizeNumberFormatterPromise=deferred;return deferred.promise}};
var waitForReferenceDataInitialization={referenceData:function(appConfigService,dataServiceG2,$q){var deferred=$q.defer();if(!appConfigService.isLoaded)appConfigService.isLoaded=deferred;else deferred.resolve(appConfigService.isLoaded);return deferred.promise}};
var waitForReferenceDataMoreInitialization={referenceData:function(appConfigService,dataServiceG2,$q){var deferred=$q.defer();if(!appConfigService.isMoreLoaded)appConfigService.isMoreLoaded=deferred;else deferred.resolve(appConfigService.isMoreLoaded);return deferred.promise}};
function searchFunctionForSalelist(dataServiceG2,searchCriteria,$q,$route,$location){var deferred=$q.defer();var saleDateInUrl=$location.search().saleDate;searchCriteria.MISC=[];if($route.current.params.yard)searchCriteria.MISC.push("yard_number:"+$route.current.params.yard);if($route.current.params.lane)searchCriteria.MISC.push("auction_lane:"+$route.current.params.lane);if(saleDateInUrl&&$route.current.params.auctionDate){saleDateInUrl=moment.utc(parseInt(saleDateInUrl));searchCriteria.MISC.push("auction_date_utc:"+
getFullDayRange(saleDateInUrl));searchCriteria.MISC.push("auction_date_type:F")}else if($route.current.params.auctionDate){var auctionDate=$route.current.params.auctionDate;searchCriteria.MISC.push("auction_date_utc:"+getFullDayRange(moment(auctionDate).utc(auctionDate)));searchCriteria.MISC.push("auction_date_type:F")}else if(saleDateInUrl=="Future"){var currentSaleDate=$location.search().recentSale;var nextSaleDate=$location.search().nextSale;var fixedAuctionDate=nextSaleDate?nextSaleDate:currentSaleDate;
if(fixedAuctionDate)searchCriteria.MISC.push("auction_date_utc:"+getFutureRange(moment(fixedAuctionDate).utc(fixedAuctionDate))+"+OR+auction_date_type:E");else searchCriteria.MISC.push("auction_date_type:E")}deferred.resolve(searchCriteria);return deferred.promise}
app.directive("tabs",function(){return{restrict:"E",transclude:true,scope:{},controller:function($scope,$element){var panes=$scope.panes=[];$scope.select=function(pane){angular.forEach(panes,function(pane){pane.selected=false});if(pane.load!==undefined)pane.load();pane.selected=true};this.select=function(pane){$scope.select(pane)};this.addPane=function(pane){if(panes.length===0)$scope.select(pane);panes.push(pane)}},template:'\x3cdiv class\x3d"tabbable"\x3e'+"\x3cul\x3e"+'\x3cli ng-repeat\x3d"pane in panes" data-uName\x3d"{{pane.uname}}" ng-class\x3d"{active:pane.selected}"\x3e'+
'\x3ca href\x3d"" ng-click\x3d"select(pane)"\x3e{{pane.tabTitle}}\x3c/a\x3e'+"\x3c/li\x3e"+"\x3c/ul\x3e"+'\x3cdiv class\x3d"tab-content" ng-transclude\x3e\x3c/div\x3e'+"\x3c/div\x3e",replace:true}});
